<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.trinet.demo.beans.Employee" table="employee">
		<id name="employee_id" column="employee_id">
			<generator class="assigned"></generator>
		</id>

		<property name="first_Name" column="first_Name"></property>
		<property name="last_name" column="last_name"></property>
		<property name="department" column="department"></property>
	</class>

</hibernate-mapping>  
